{
	"type" : "Intel NUC",
	"id" : 1,
	"remotehost" : "autograder.nesl.ucla.edu",
	"remoteport" : 80,
	"localport" : 8889,
	"testbed_type": "time_sync_firing_playground",
	"tester" : {
		"type": "ARM Cortex-M4",
		"path": "/dev/ttyUSB0",
		"tickrate": 1e-6,
		"maxevents": 7000,
		"timeresolution": 1e-6
	},
	"duts" : [
		{
			"name": "mbed1",
			"type": "mbed",
			"id" : 1,
			"mount" : "/media/embed-autograder/DUT/",
			"dev_path": "/dev/sdc",
			"usb_path": "/dev/ttyACM0",
			"do_nothing_firmware": "../do_nothing_firmware/NUCLEO_F746ZG.bin"
		}
	],
	"wires" : [
		{
			"name": "wire1",
			"first": {
				"dutid": 1,
				"pin": "D0"
			},
			"second": {
				"dutid": 1,
				"pin": "D3"
			}
		}
	]
}
